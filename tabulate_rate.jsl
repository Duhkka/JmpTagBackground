Names Default To Here( 1 );Clear Symbols();win = New Window( "Select Data Table",	Outline Box( "Select Data Table",		V List Box(			Lineup Box( N Col( 2 ), 					Text Box( "Click on Table" ),				Button Box( "Done", 						dt = Current Data Table();					obj = Tabulate(					Add Table(						Row Table(							Grouping Columns( :Cell)						)					)					);					ll= obj << make into data table;					obj << close window;					dt2 = Current Data Table();					New Window( "Total Time (Minutes)", <<Modal, Outline Box("Total Time (Minutes)", totaltime_box = Number Edit Box( 200 ), Button Box( "OK", totaltime = totaltime_box << get ),), );					dt2 << New Table Variable( "Total Time", totaltime );						//obj << New Column("Rate");					ll << New Column("Rate", Numeric, "Continuous", Formula(N / :Name( "Total Time" )));					show (ll);					),					),		),	),);wait(10);win << close window;dt2 << Distribution(	Continuous Distribution( Column( :Rate ), Fit Distribution( Exponential ) ),	SendToReport(		Dispatch(			{"Distributions", "Rate"},			"1",			ScaleBox,			{Min( 0 ), Max( 2 ), Inc( 0.1 ), Minor Ticks( 1 )}		)	))