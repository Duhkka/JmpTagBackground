Names Default To Here( 1 );Clear Symbols();win = New Window( "Select Data Table",	Outline Box( "Select Data Table",		Button Box( "Okay", 						dt = Current Data Table();				New Window( "Cutoff", <<Modal, Outline Box("Cutoff (s)", cutoff_box = Number Edit Box( 0.04 ), Button Box( "OK", cutoff = cutoff_box << get ),), );			dt << New Table Variable( "Duration Cutoff", cutoff );			dt << New Column( "Duration Marker",				numeric,				nominal,				formula(					If(:Name("Dwell Duration (S)") < :Name( "Duration Cutoff" ), 1, 2)						)					);				),			),					);		wait(5);win << close window;