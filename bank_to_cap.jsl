Names Default To Here( 1 );Clear Symbols();win = New Window( "Select Data Table",	Outline Box( "Select Data Table",		V List Box(			Lineup Box( N Col( 2 ), 								Text Box( "Click on Table" ),				Button Box( "Done", 								dt = Current Data Table();				dt << New Column("NCAP1",					Numeric,					continuous,					formula(Starts With(:Cell, "b0") | Starts With(:Cell, "b5") | Starts With(:Cell, "b10") | Starts With(:Cell, "b15"))					);					dt << New Column("NCAP2",					Numeric,					continuous,					formula(Starts With(:Cell, "b1") | Starts With(:Cell, "b6") | Starts With(:Cell, "b11") | Starts With(:Cell, "b12"))					);dt << New Column("NCAP3",					Numeric,					continuous,					formula(Starts With(:Cell, "b2") | Starts With(:Cell, "b7") | Starts With(:Cell, "b8") | Starts With(:Cell, "b13"))					);dt << New Column("NCAP4",					Numeric,					continuous,					formula(Starts With(:Cell, "b3") | Starts With(:Cell, "b4") | Starts With(:Cell, "b9") | Starts With(:Cell, "b14"))					);dt << New Column("Bank",					numeric,					nominal,					formula(If(:Name("NCAP1") == 1, 1,							If(:Name("NCAP2") == 1, 2,							If(:Name("NCAP3") == 1, 3,							If(:Name("NCAP4") == 1, 4,								)))))					);),								),				),			),		);wait(3);win << close window;