Names Default To Here( 1 );Clear Symbols();win = New Window( "Select Data Table",	Outline Box( "Select Data Table",		Button Box( "Okay", 						dt = Current Data Table();				dt << New Column( "Wait Time (s)",				numeric,				nominal,				formula(					If(:Cell[Row(),Empty()] == :Cell[Row() - 1,Empty()], :Name("Dwell Start (s)")[Row(),Empty()] - :Name("Dwell Stop (s)")[Row() - 1,Empty()], Empty())						)					);				),			),					);		wait(5);win << close window;